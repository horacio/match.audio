        }
      ]
    ]
  },
  "dependencies": {
    "bluebird": "^2.3.11",
    "body-parser": "~1.8.1",
    "compression": "^1.2.2",
    "connect-flash": "^0.1.1",
    "connect-browserify": "^3.2.1",
    "cookie-parser": "~1.3.3",
    "crypto-js": "^3.1.2-5",
    "debug": "~2.0.0",
    "ejs": "~0.8.5",
    "express": "~4.9.0",
    "express-session": "^1.9.2",
    "helmet": "^0.5.2",
    "morgan": "~1.3.0",
    "node-jsx": "^0.12.4",
    "node-uuid": "^1.4.2",
    "promised-mongo": "^0.11.1",
    "rdio": "^1.5.2",
    "react": "^0.12.1",
    "react-google-analytics": "^0.2.0",
    "react-router": "^0.11.6",
    "serve-favicon": "~2.1.3",
    "spotify": "^0.3.0",
