{
  "name" : "Match Audio",
  "version" : "0.2.2",
  "description" : "Match Audio makes sharing from music services better.",
  "background" : {
    "persistent": false,
    "scripts": [ "background.js" ]
  },
  "page_action" : {
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "permissions" : [ "declarativeContent", "https://match.audio/" ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
